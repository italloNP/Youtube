    <style>
      /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
      #map {
        height: 80%;
      }
      /* Optional: Makes the sample page fill the window. */
      html, body {
        height: 80%;
        margin: 5;
        padding: 5;
      }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -7.135409, lng: -34.873553},
          zoom: 18
        });
      }
    </script>
 <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBRFSZ7cXClY-Fdhjx2rc70ycklFjhWIPA&callback=initMap">
    </script>
